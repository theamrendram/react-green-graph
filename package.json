{
  "name": "@theamrendram/react-green-graph",
  "version": "0.0.1",
  "description": "A simple React component to embed GitHub-style contribution graphs in your web app.",
  "main": "dist/green-graph.umd.js",
  "module": "dist/green-graph.es.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/green-graph.es.js",
      "require": "./dist/green-graph.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "test": "echo \"No tests specified\"",
    "build": "vite build",
